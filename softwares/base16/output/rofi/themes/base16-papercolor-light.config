! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #f3f3f3, #f3f3f3, #f3f3f3

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #f3f3f3, #4d4d4c, #f3f3f3, #f3f3f3, #262626
rofi.color-active:  #f3f3f3, #005f87, #f3f3f3, #f3f3f3, #005f87
rofi.color-urgent:  #f3f3f3, #8959a8, #f3f3f3, #f3f3f3, #8959a8

! Set the desired separator style
rofi.separator-style: solid
